{
  "name": "mini-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tunnel": "vk-tunnel --insecure=1 --http-protocol=http --ws-protocol=ws --host=0.0.0.0 --port=5173",
    "deploy": "vite build && vk-miniapps-deploy"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "@vkontakte/icons": "^3.1.1",
    "@vkontakte/vk-bridge": "^2.13.0",
    "@vkontakte/vk-bridge-react": "^1.0.1",
    "@vkontakte/vk-mini-apps-router": "^1.7.6",
    "@vkontakte/vkui": "^7.6.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vkontakte/vk-miniapps-deploy": "^0.1.6",
    "@vkontakte/vk-tunnel": "^0.2.8",
    "eruda": "^3.0.1",
    "esbuild": "~0.20.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "terser": "^5.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
